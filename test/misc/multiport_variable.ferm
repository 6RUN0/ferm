# test contributed by Bill Goudie
# http://bugs.debian.org/398867

# ferm 1.2.1 used to clear variables when used as parameter in
# multiport.

@def $PORTS = (80 443);

table filter {
  chain INPUT {
    proto tcp mod multiport destination-ports $PORTS ACCEPT;
  }

  chain OUTPUT {
    proto tcp mod multiport destination-ports $PORTS ACCEPT;
  }
}
